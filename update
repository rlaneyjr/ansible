#!/bin/sh

cp repo-osa/playbooks/common-tasks/dynamic-address-fact.yml osa/playbooks/common-tasks/dynamic-address-fact.yml
cp repo-osa/lib/filesystem.py osa/lib/filesystem.py
cp repo-osa/lib/manage.py osa/lib/manage.py
cp repo-osa/playbooks/common-tasks/haproxy-endpoint-manage.yml osa/playbooks/common-tasks/haproxy-endpoint-manage.yml
cp repo-osa/playbooks/galera-install.yml osa/playbooks/galera-install.yml
cp repo-osa/playbooks/inventory/group_vars/galera_all.yml osa/playbooks/inventory/group_vars/galera_all.yml
cp repo-osa/playbooks/inventory/group_vars/gnocchi_all.yml osa/playbooks/inventory/group_vars/gnocchi_all.yml
cp repo-osa/playbooks/os-cinder-install.yml osa/playbooks/os-cinder-install.yml
cp repo-osa/playbooks/os-gnocchi-install.yml osa/playbooks/os-gnocchi-install.yml
cp repo-osa/playbooks/os-neutron-install.yml osa/playbooks/os-neutron-install.yml
cp repo-osa/playbooks/os-nova-install.yml osa/playbooks/os-nova-install.yml
cp repo-osa/requirements.txt osa/requirements.txt
cp repo-osa/tests/bootstrap-aio.yml osa/tests/bootstrap-aio.yml
cp repo-osa/tests/roles/bootstrap-host/templates/user_variables.aio.yml.j2 osa/tests/roles/bootstrap-host/templates/user_variables.aio.yml.j2
cp repo-osa/tests/test_manage.py osa/tests/test_manage.py
Only in repo-osa: .git
Only in repo-osa: .gitignore
Only in repo-osa: .gitreview
